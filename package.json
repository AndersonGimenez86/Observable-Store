{
  "name": "@codewithdan/observable-store",
  "version": "2.1.0",
  "author": "Dan Wahlin",
  "description": "Observable Store provides a simple way to store and retrieve state in Angular, React, Vue.js and other front-end applications.",
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": {
    "url": "https://github.com/danwahlin/observable-store"
  },
  "keywords": [
    "store",
    "state management",
    "Angular store",
    "React store",
    "Vue.js store",
    "state store",
    "JavaScript store",
    "Front-end store"
  ],
  "scripts": {
    "tsc": "tsc",
    "build-store": "rm -rf ./dist && tsc --build ./modules/observable-store/tsconfig.json",
    "build-store:w": "tsc  --build ./modules/observable-store/tsconfig.json --watch",
    "build-store-extensions": "rm -rf ./dist-extensions && tsc --build ./modules/observable-store-extensions/tsconfig.json",
    "build-store-extensions:w": "tsc  --build ./modules/observable-store-extensions/tsconfig.json --watch",
    "test": "jasmine-ts --project ./spec/tsconfig.json --config=./spec/jasmine.json",
    "test:w": "nodemon --ext ts --exec 'jasmine-ts --project ./spec/tsconfig.json --config=./spec/jasmine.json'"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "peerDependencies": {
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "@types/history": "^4.7.3",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "rxjs": "^6.4.0"
  }
}
